/* SEPTEMBER 2: REMOVE SPACE FROM STRING */

/* REMOVE SPACES FROM BEGINING TO THE END OF STRING NOT AFTER */
SELECT TRIM ('    STRING   ');
/* REMOVE FROM RIGHT OF THE STRING */
SELECT RTRIM ('    STRING   ');
/* REMOVE FROM LEFT OF THE STRING */
SELECT LTRIM ('    STRING   ');

/* REMOVE SPECIAL CHARACTER */
SELECT LTRIM ('....STRING......','.');




